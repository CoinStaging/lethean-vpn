port {port}
proto {proto}
dev tun
dev-node {tunnode}
persist-tun
persist-key
verify-client-cert none
management-client-auth
<ca>
{f_ca}
</ca>
<cert>
{f_crt}
</cert>
<key>
{f_key}
</key>
cd {workdir}
script-security 3
dh {f_dh}
topology subnet
server {iprange} {ipmask}
keepalive 30 120
reneg-sec {reneg}
<tls-auth>
{f_ta}
</tls-auth>
key-direction 0
auth SHA256
management {mgmt_sock}
management-hold
management-up-down
management-query-passwords
cipher AES-256-CBC
#user {user}
#group {group}
status {f_status}
float
tun-mtu {mtu}
mssfix {mssfix}
